# マッチングアプリLP設定

# 対象オーディエンス (demand: 需要側, supply: 供給側)
NEXT_PUBLIC_TARGET_AUDIENCE=demand

# ウェイティングリスト連携設定
# 連携タイプ (email, notion, spreadsheet)
NEXT_PUBLIC_INTEGRATION_TYPE=email

# メール送信設定
EMAIL_TO=admin@matching-app.com

# Notion連携設定（NEXT_PUBLIC_INTEGRATION_TYPE=notionの場合）
NOTION_DATABASE_ID=your_notion_database_id
NOTION_TOKEN=your_notion_integration_token

# Googleスプレッドシート連携設定（NEXT_PUBLIC_INTEGRATION_TYPE=spreadsheetの場合）
GOOGLE_SPREADSHEET_ID=your_google_spreadsheet_id
GOOGLE_SERVICE_ACCOUNT_EMAIL=your_service_account_email
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY\n-----END PRIVATE KEY-----\n"

# サイト設定
NEXT_PUBLIC_SITE_NAME=マッチングアプリ
NEXT_PUBLIC_DOMAIN=matching-app.com

# 分析設定（任意）
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
NEXT_PUBLIC_FB_PIXEL_ID=1234567890
